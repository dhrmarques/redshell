<div class="page-header">
	<h1>Bem Vindo ao Coopel - Serviços Estruturais</h1>
</div>
<div class="welcome">
	<h1>Coopel - Serviços Estruturais</h1>
</div>
<ul>
<% @entity_classes.each do |ec| %>
	<li>
		<% cls = ec.to_s.classify.constantize %>
		<%= link_to "<i class=\"fa fa-#{cls.icon}\"></i> #{cls.label}".html_safe, polymorphic_path(ec), class: "btn btn-info" %>
	</li>
<% end %>
</ul>
